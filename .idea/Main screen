package com.example.tablelayout

import android.content.Intent
import androidx.appcompat.app.AppCompatActivity
import android.os.Bundle
import android.widget.Button
import android.widget.EditText
import android.widget.TextView

class Main3Activity : AppCompatActivity() {
    override fun onCreate(savedInstanceState: Bundle?) {
        super.onCreate(savedInstanceState)
        setContentView(R.layout.activity_main3)

     val weekly=findViewById<TextView>(R.id.textView5)
     val calculate=findViewById<TextView>(R.id.textView10)
      val days=findViewById<TextView>(R.id.textView6)
     val input=findViewById<EditText>(R.id.editTextText3)
     val average=findViewById<EditText>(R.id.editTextText2)
     val clear=findViewById<Button>(R.id.button4)
     val next=findViewById<Button>(R.id.button2)
      val exit=findViewById<Button>(R.id.button3)

        button?.setOnClickListener {
            val intent = Intent(this, MainActivity3::class.java)
            startActivity(intent)
        
        
        }
        
        }
}
